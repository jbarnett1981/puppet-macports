{
  "name": "puppet-macports",
  "version": "0.2.0",
  "author": "Julian Barnett",
  "summary": "Puppet Module for managing MacPorts",
  "license": "GPLv3+",
  "source": "https://github.com/jbarnett1981/puppet-macports.git",
  "project_page": "https://github.com/jbarnett1981/puppet-macports.git",
  "issues_url": "https://github.com/jbarnett1981/puppet-macports/issues",
  "description": "Puppet Module to install/update MacPorts on OS X",
  "operatingsystem_support": [
    {
      "operatingsystem": "Darwin",
      "operatingsystemrelease": [
        "12.x",
        "13.x",
        "14.x",
	"15.x",
	"16.x"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.1.0"
    }
  ]
}
